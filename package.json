{
  "name": "ts-jmbg",
  "version": "0.1.1",
  "description": "JMBG tools in typescript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "test:watch": "jest --config jestconfig.json --watch",
    "coverage": "npm test -- --coverage",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borko-rajkovic/ts-jmbg.git"
  },
  "author": "Borko Rajković",
  "license": "MIT",
  "keywords": [
    "JMBG",
    "ЈМБГ",
    "ЕМБГ",
    "Serbia",
    "Croatia",
    "Macedonia",
    "Slovenia",
    "Montenegro",
    "Yugoslavia"
  ],
  "bugs": {
    "url": "https://github.com/borko-rajkovic/ts-jmbg/issues"
  },
  "homepage": "https://github.com/borko-rajkovic/ts-jmbg#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "coveralls": "^3.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "lib/**/*"
  ]
}
